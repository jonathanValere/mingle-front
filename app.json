{
  "expo": {
    "name": "mingle-front",
    "slug": "mingle-front",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-mingle.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-mingle.png",
      "resizeMode": "contain",
      "backgroundColor": "#563D8F"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
